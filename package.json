{
  "name": "@adobe/aio-cli-plugin-jwt-auth",
  "version": "2.0.3",
  "author": "Adobe Inc.",
  "description": "JWT Auth Plugin for the Adobe I/O CLI",
  "bugs": "https://github.com/adobe/aio-cli-plugin-jwt-auth/issues",
  "dependencies": {
    "@adobe/aio-lib-core-config": "^1.0.15",
    "@adobe/jwt-auth": "^0.3.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.1.2",
    "debug": "^4.1.0",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/plugin-help": "^2.1.4",
    "@oclif/test": "^1",
    "acorn": "^7.0.0",
    "chalk": "^3.0.0",
    "codecov": "^3.2.0",
    "eslint": "^6.0.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "jest-haste-map": "^24.5.0",
    "jest-junit": "^9.0.0",
    "jest-resolve": "^24.5.0",
    "stdout-stderr": "^0.1.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/adobe/aio-cli-plugin-jwt-auth",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "./bin/run",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "jwt-auth": {
        "description": "Gets the access token for an integration for the Adobe I/O Runtime"
      }
    }
  },
  "main": "src/index.js",
  "repository": "adobe/aio-cli-plugin-jwt-auth",
  "scripts": {
    "test": "npm run unit-tests",
    "pretest": "npm run eslint",
    "eslint": "eslint src test",
    "unit-tests": "jest --ci",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  }
}
